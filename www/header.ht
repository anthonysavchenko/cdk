//Orientation function
function kit_resize(kit_resize_tmp){
	//document.getElementById("id_load_block").style.display = "block";

	browser_width = screen.width;
	browser_height = screen.height;

	if(device.mobile() == true ){
		if(kit_resize_tmp == 1){
			document.getElementById("id_main_edit_download").style.display = "none";
			mm_hei_koef = browser_width/900;
			document.getElementById("id_main_png").style.zoom = mm_hei_koef;

			mm_hei = (parseInt(document.getElementById("id_main_png").offsetHeight) * mm_hei_koef) + 15;
			document.getElementById("id_main_edit").style.display = "block";
			document.getElementById("id_main_edit").style.marginTop = mm_hei + "px";
			color_height = browser_height - (parseInt(document.getElementById("id_main_png").offsetHeight) * mm_hei_koef);
			document.getElementById("id_set_color_block").style.height = color_height + 'px';
		}else if(kit_resize_tmp == 2){
			document.getElementById("id_main_edit").style.display = "none";
			mm_hei_koef = browser_height/575;
			document.getElementById("id_main_png").style.zoom = mm_hei_koef;
			document.getElementById("id_main_edit_download").style.display = "block";
		}
	}else if(device.tablet() == true){
		if(kit_resize_tmp == 4){
			document.getElementById("id_main_edit").style.display = "none";
			mm_hei_koef = browser_height/575;
			document.getElementById("id_main_png").style.zoom = mm_hei_koef;
			document.getElementById("id_main_edit_download").style.display = "block";
		}
	}
	document.getElementById("id_load_block").style.display = "none";
}






















//Mobile
function open_color_set(tmp_main_color_reg){
	main_color_reg = tmp_main_color_reg;
	if(main_color_reg == 1){
		document.getElementById("id_set_color_block_text").innerHTML = "Цвет обоев";
	}else if(main_color_reg == 2){
		document.getElementById("id_set_color_block_text").innerHTML = "Цвет потолка";
	}else if(main_color_reg == 3){
		document.getElementById("id_set_color_block_text").innerHTML = "Цвет полов";
	}else if(main_color_reg == 4){
		document.getElementById("id_set_color_block_text").innerHTML = "Цвет 2-го уровня";
	}
	document.getElementById("id_set_color_block").style.display = "block";
	$("#id_set_color_block").animate({bottom: '0'},500, function() {

 	});	
}



function close_color_set(){
	$("#id_set_color_block").animate({bottom: '-100%'},500, function() {
		document.getElementById("id_set_color_block").style.display = "none";
 	});	
}



//Computer
function open_color_set_mini(tmp_main_color_reg){

	if(document.getElementById("id_potolok_list_ex").style.display == "block"){
		document.getElementById("id_potolok_list_ex").style.display = "none";
		$("#id_potolok_example").toggleClass("oboi_example_rot");
	}

	if(document.getElementById("id_oboi_list_ex").style.display == "block"){
		document.getElementById("id_oboi_list_ex").style.display = "none";
		$("#id_oboi_example").toggleClass("oboi_example_rot");
	}

	document.getElementById("id_set_color_block_mini").focus();

	if(main_color_reg != tmp_main_color_reg){
		main_color_reg = tmp_main_color_reg;
		document.getElementById("id_open_color_set_mini1").innerHTML = "";
		document.getElementById("id_open_color_set_mini2").innerHTML = "";
		document.getElementById("id_open_color_set_mini3").innerHTML = "";
		document.getElementById("id_open_color_set_mini4").innerHTML = "";
		document.getElementById("id_set_color_block_mini").classList.remove('rott');

		if(device.mobile() == true ){
			// Mobile - open color palitra
				main_color_reg = tmp_main_color_reg;
				if(main_color_reg == 1){
					document.getElementById("id_set_color_block_mini_text").innerHTML = "Цвет стен";
				}else if(main_color_reg == 2){
					document.getElementById("id_set_color_block_mini_text").innerHTML = "Цвет потолка";
				}else if(main_color_reg == 3){
					document.getElementById("id_set_color_block_mini_text").innerHTML = "Цвет полов";
				}else if(main_color_reg == 4){
					document.getElementById("id_set_color_block_mini_text").innerHTML = "Цвет 2-го уровня";
				}
				document.getElementById("id_set_color_block_mini").style.display = "block";
				$("#id_set_color_block_mini").animate({bottom: '0'},500, function(){
			 	});	
		}else if(device.tablet() == true ){
			// Tablet - open color palitra

		}else{
			// Computer - open color palitra
			if(main_color_reg == 1){
				document.getElementById("id_set_color_block_mini").style.top = "96px";
				document.getElementById("id_open_color_set_mini1").innerHTML = "X";
				document.getElementById("id_set_color_block_mini_text").innerHTML = "Цвет стен";
			}else if(main_color_reg == 2){
				document.getElementById("id_set_color_block_mini").style.top = "194px";
				document.getElementById("id_open_color_set_mini2").innerHTML = "X";
				document.getElementById("id_set_color_block_mini_text").innerHTML = "Цвет потолка";
			}else if(main_color_reg == 3){
				document.getElementById("id_set_color_block_mini").style.top = "260px";
				document.getElementById("id_set_color_block_mini").classList.add('rott');
				document.getElementById("id_open_color_set_mini3").innerHTML = "X";
				document.getElementById("id_set_color_block_mini_text").innerHTML = "Цвет пола";
			}else if(main_color_reg == 4){
				document.getElementById("id_set_color_block_mini").style.top = "43px";
				document.getElementById("id_set_color_block_mini").classList.add('rott');
				document.getElementById("id_open_color_set_mini4").innerHTML = "X";
				document.getElementById("id_set_color_block_mini_text").innerHTML = "Цвет 2-го уровня";
			}
			document.getElementById("id_set_color_block_mini").style.display = "block";
		}
	}else{
		main_color_reg = 0;
		document.getElementById("id_set_color_block_mini").style.display = "none";
		document.getElementById("id_open_color_set_mini1").innerHTML = "";
		document.getElementById("id_open_color_set_mini2").innerHTML = "";
		document.getElementById("id_open_color_set_mini3").innerHTML = "";
		document.getElementById("id_open_color_set_mini4").innerHTML = "";
	}
}




























function screen_show(tmp_screen){
	document.getElementById("id_screen_computer").style.display = "none";	
	document.getElementById("id_screen_tablet").style.display = "none";	
	document.getElementById("id_screen_phone").style.display = "none";	

	document.getElementById("id_screen_computer_s").classList.remove("active");
	document.getElementById("id_screen_tablet_s").classList.remove("active");
	document.getElementById("id_screen_phone_s").classList.remove("active");

	if(tmp_screen == 0){
		document.getElementById("id_screen_computer").style.display = "block";
		document.getElementById("id_screen_computer_s").classList.add("active");
	}else if(tmp_screen == 1){
		document.getElementById("id_screen_tablet").style.display = "block";
		document.getElementById("id_screen_tablet_s").classList.add("active");
	}else if(tmp_screen == 2){
		document.getElementById("id_screen_phone").style.display = "block";	
		document.getElementById("id_screen_phone_s").classList.add("active");
	}
}



function open_cl(tmp_open_cl){
	document.getElementById("id_vib1").style.background = "#ffdd55";
	document.getElementById("id_vib2").style.background = "#ffdd55";
	document.getElementById("id_vib3").style.background = "#ffdd55";

	if(tmp_open_cl == 1){
		document.getElementById("id_send_klient_desc").innerHTML = "Базовый";
		document.getElementById("id_send_klient").style.left = "169px";	
		document.getElementById("id_vib1").style.background = "#ccc";	
	}else if(tmp_open_cl == 2){
		document.getElementById("id_send_klient_desc").innerHTML = "Оптимальный";
		document.getElementById("id_send_klient").style.left = "367px";	
		document.getElementById("id_vib2").style.background = "#ccc";
	}else if(tmp_open_cl == 3){
		document.getElementById("id_send_klient_desc").innerHTML = "Профессиональный";
		document.getElementById("id_send_klient").style.left = "570px";
		document.getElementById("id_vib3").style.background = "#ccc";	
	}
	document.getElementById("id_send_klient").style.display = "block";
}



function close_cl(){
	document.getElementById("id_send_klient").style.display = "none";	
	document.getElementById("id_vib1").style.background = "#ffdd55";
	document.getElementById("id_vib2").style.background = "#ffdd55";
	document.getElementById("id_vib3").style.background = "#ffdd55";
	document.getElementById("id_kl_name").value = "";
	document.getElementById("id_kl_mail").value = "";
	document.getElementById("id_kl_site").value = "";
	document.getElementById("id_send_klient_b").innerHTML = "Отправить";
}



function send_za(){
	var chet='';
	var kl_desc_text = document.getElementById("id_send_klient_desc").innerHTML;
	var kl_name_text = document.getElementById("id_kl_name").value;
	var kl_email_text = document.getElementById("id_kl_mail").value;
	var kl_site_text = document.getElementById("id_kl_site").value;

	if(kl_email_text == ""){
		alert('Введите ваш E-Mail');
	}else{
		document.getElementById("id_send_klient_b").innerHTML = "Отправление...";

		chet='';
		chet += "Пакет: <b>" + kl_desc_text + "</b><br /><br />";
		chet += "Имя: <b>" + kl_name_text + "</b><br />";
		chet += "Почта: <b>" + kl_email_text + "</b><br />";
		chet += "Сайт: <b>" + kl_site_text + "</b><br />";

		$.ajax({
			type: "POST",
			url: "php/send_klient.php",
			data: {'message': chet},
			success: function(text) {
				close_cl();
				alert(text);
			}
		});
	}	
}









